import java.util.Scanner;

public class exercicio5 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int f=0;
		for(int i=0; i<n; i++) {
			
			f = f*1;
			
		}
		System.out.println(f);
		sc.close();
	}

}
